" Hide Toolbar
set guioptions-=T
set guioptions-=r
set guioptions-=m
set guioptions-=L
set guioptions-=b
set guioptions-=e

" Maximize window size when startup
set lines=999 columns=999

set guifont=Consolas\ 13

set t_ut=
set background=dark

colorscheme codedark
set t_Co=256

hi SpecialKey guifg=#585858
